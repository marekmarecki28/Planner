<html t:type="layout" title="Plan"
      xmlns:t="http://tapestry.apache.org/schema/tapestry_5_4.xsd"
      xmlns:p="tapestry:parameter">


	<t:Form t:id="calendarForm">
		<input class="btn btn-lg btn-primary btn-block" t:id="up" t:type="Submit" value="Poprzedni tydzień"/>
		<br/>
		<table t:type="grid" t:source="hotelCalendars" t:row="hotelCalendar" t:add="users">
			<p:usersCell>
               <t:grid t:source="hotelCalendar.listUserCalendarDescr" t:reorder="fullName,workStart,workEnd,description" t:exclude="calendarId,employeeId"/>
            </p:usersCell>
        </table>
		<br/>
		<input class="btn btn-lg btn-primary btn-block" t:id="down" t:type="Submit" value="Następny tydzień"/>
	</t:Form>
	

</html>