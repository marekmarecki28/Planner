<html t:type="layout" title="Plan"
      xmlns:t="http://tapestry.apache.org/schema/tapestry_5_4.xsd"
      xmlns:p="tapestry:parameter">


	<h2>Wybierz pracownika:</h2>
	<t:Form>
    	<t:select t:id="employeeMenu" value="employee" model="personsModel" t:encoder="employeeEncoder"/>
    	<br/>
    	<input class="btn btn-lg btn-primary btn-block" t:id="submit" t:type="Submit" value="Submit"/>
	</t:Form>
	
	<br/>
	
	<t:If t:test="submitted">
		<t:BeanDisplay t:object="employee"/>
	
	<br/>
	<t:Form>
	<input class="btn btn-lg btn-primary btn-block" t:id="up" t:type="Submit" value="^"/>
	<br/>
	<table t:type="grid" t:source="calendarsWeek" t:row="calendar" t:exclude="userId"/>
	<br/>
	<input class="btn btn-lg btn-primary btn-block" t:id="down" t:type="Submit" value="v"/>
	</t:Form>
	${week}
	${year}
	</t:If>

</html>